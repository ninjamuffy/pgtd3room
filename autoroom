-- Create a party with specified arguments
local createArgs = {
    [1] = {
        ["Difficulty"] = 1,
        ["Modifier1"] = "BulletSponge",
        ["Code"] = "yooi9s",
        ["Map"] = "Inside The Code",
        ["ModNames"] = "Bullet Sponges + Stronger Enemies",
        ["Modifier2"] = "EnemyHealth",
        ["Modifier3"] = ""
    }
}

-- Fire the createparty event
game:GetService("ReplicatedStorage").EventMatchmakingRemotes.createparty:FireServer(unpack(createArgs))

-- Wait for a short period to allow the server to process the request
wait(1)

-- Start the created party
local party = game:GetService("ReplicatedStorage").EventParties:FindFirstChild("")
if party then
    local startArgs = {
        [1] = party
    }

    -- Fire the start event
    game:GetService("ReplicatedStorage").EventMatchmakingRemotes.start:FireServer(unpack(startArgs))
else
    warn("Party not found.")
end
